{
  "name": "get-version",
  "version": "1.1.4",
  "description": "Get versions list for npm, node and iojs",
  "main": "index.js",
  "bin": {
    "get-version": "./bin/get-version"
  },
  "scripts": {
    "lint": "mod-standard",
    "test": "lab -c",
    "posttest": "npm run lint",
    "coverage": "lab -r html -o coverage/index.html -r lcov -o coverage/lcov.info"
  },
  "keywords": [
    "npm",
    "node",
    "version",
    "iojs"
  ],
  "author": "Alex Padgett <alex@modulus.io>",
  "maintainers": [
    "Alex Padgett <alex@modulus.io>",
    "Jack Boberg <jack@modulus.io>",
    "Matt Hernandez <matt@modulus.io>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/onmodulus/get-version.git"
  },
  "license": "MIT",
  "dependencies": {
    "lodash": "3.6.0",
    "minimist": "1.1.1",
    "q": "1.2.0",
    "request": "2.54.0",
    "require-dir": "^0.3.0",
    "semver": "4.3.1",
    "winston": "0.9.0"
  },
  "devDependencies": {
    "@modulus/standard": "0.2.x",
    "code": "1.x.x",
    "lab": "5.x.x",
    "proxyquire": "1.4.0",
    "sinon": "1.14.1"
  }
}
