{
  "name": "get-version",
  "version": "1.1.3",
  "description": "Get versions list for npm, node and iojs",
  "main": "index.js",
  "bin": {
    "get-version": "./bin/get-version"
  },
  "scripts": {
    "lint": "mod-standard",
    "test": "lab -c",
    "posttest": "npm run lint",
    "coverage": "lab -r html -o coverage/index.html -r lcov -o coverage/lcov.info"
  },
  "keywords": [
    "npm",
    "node",
    "version",
    "iojs"
  ],
  "author": "Alex Padgett <alex@modulus.io>",
  "maintainers": [
    "Alex Padgett <alex@modulus.io>",
    "Jack Boberg <jack@modulus.io>",
    "Matt Hernandez <matt@modulus.io>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/onmodulus/get-version.git"
  },
  "license": "MIT",
  "dependencies": {
    "lodash": "4.17.2",
    "minimist": "1.2.0",
    "q": "1.4.1",
    "request": "2.79.0",
    "require-dir": "0.3.1",
    "semver": "5.3.0",
    "winston": "2.3.0"
  },
  "devDependencies": {
    "@modulus/standard": "2.0.0",
    "code": "4.0.0",
    "lab": "11.2.1",
    "proxyquire": "1.7.10",
    "sinon": "1.17.6"
  }
}
